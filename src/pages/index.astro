---
import Layout from "../layouts/Layout.astro";

const data = await fetch("http://localhost:3000/api/handler")
	.then((response) => response.text())
	.catch((err) => {
		throw new Error(`Couldn't build library ${err}`);
	});
---

<Layout title="Welcome to Astro.">
	<main>Hello</main>
	<p>
		{data}
	</p>
</Layout>
